APP_NAME="Gestion Parcours USMBA"
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_TIMEZONE=Africa/Casablanca
APP_URL=http://localhost

APP_LOCALE=fr
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=fr_FR

APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=database

PHP_CLI_SERVER_WORKERS=4

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=gestion_parcours_usmba
DB_USERNAME=root
DB_PASSWORD=

# Pour les environnements sans Redis (hébergement mutualisé, Railway sans addon Redis)
# Utilisez ces valeurs au lieu de 'redis'
SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local

# Configuration du cache - Utiliser 'file' si Redis n'est pas disponible
# Options: file, database, redis
CACHE_STORE=file

# Configuration des files d'attente - Utiliser 'database' ou 'sync' si Redis n'est pas disponible
# Options: sync, database, redis
# Note: Si vous utilisez 'database', exécutez: php artisan queue:table && php artisan migrate
QUEUE_CONNECTION=sync

MEMCACHED_HOST=127.0.0.1

# Configuration Redis (optionnel - décommentez si Redis est disponible)
# REDIS_CLIENT=phpredis
# REDIS_HOST=127.0.0.1
# REDIS_PASSWORD=null
# REDIS_PORT=6379

MAIL_MAILER=log
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=null
MAIL_FROM_ADDRESS="noreply@usmba.ac.ma"
MAIL_FROM_NAME="${APP_NAME}"

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

# Configuration des administrateurs
# Liste des emails autorisés à accéder à la section admin (séparés par des virgules)
# Les utilisateurs avec role='admin' dans la BDD ont aussi accès
ADMIN_EMAILS=Abdelbadie.Khoubiza@usmba.ac.ma

VITE_APP_NAME="${APP_NAME}"
